{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "noEmit": true
    },
    "include": ["api/**/*.ts", "server/**/*.ts"],
    "exclude": ["node_modules"]
}